CREATE TABLE IF NOT EXISTS public.tabla_temporal
(
    id_alumno integer NOT NULL,
    dni integer NOT NULL,
    nombre character(35) COLLATE pg_catalog."default",
    apellido character(35) COLLATE pg_catalog."default" NOT NULL,
    email character(50) COLLATE pg_catalog."default" NOT NULL,
    exp_sql boolean DEFAULT false,
    exp_no_sql boolean DEFAULT false,
    id_localidad integer,
    id_profesion integer
)
TABLESPACE pg_default;

ALTER TABLE IF EXISTS public.tabla_temporal
    OWNER to postgres;

TRUNCATE tabla_temporal;

COPY tabla_temporal
FROM '/home/the14th/Downloads/alumno_mock.csv'
DELIMITER ','
CSV HEADER;

INSERT INTO public.alumno SELECT * FROM tabla_temporal;

SELECT * FROM public.alumno;
